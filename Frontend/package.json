{
  "name": "wiselife",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "apexcharts": "^3.19.2",
    "axios": "^0.19.0",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.3",
    "core-js": "^2.6.5",
    "jwt-decode": "^2.2.0",
    "moment": "^2.26.0",
    "node-sass": "^4.13.0",
    "quill": "^1.3.6",
    "sockjs-client": "^1.4.0",
    "vue": "^2.6.10",
    "vue-apexcharts": "^1.5.3",
    "vue-carousel": "^0.18.0",
    "vue-chartjs": "^3.5.0",
    "vue-js-modal": "^1.3.33",
    "vue-jwt-decode": "^0.1.0",
    "vue-kakao-login": "^1.0.5",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.7",
    "vue-spinner": "^1.0.3",
    "vue-spinners": "^1.0.2",
    "vue-text-editor": "^1.1.1",
    "vue-youtube": "^1.4.0",
    "vue-youtube-embed": "^2.2.2",
    "vue2-editor": "^2.10.2",
    "vuetify": "^2.0.0",
    "vuex": "^3.1.1",
    "webstomp-client": "^1.2.6",
    "youtube-api-search": "0.0.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.10.0",
    "@vue/cli-plugin-eslint": "^3.10.0",
    "@vue/cli-service": "^3.10.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.17.4",
    "sass-loader": "^7.3.1",
    "vue-cli-plugin-vuetify": "^0.6.1",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2",
    "@babel/core": "^7.7.5",
    "babel-jest": "^24.9.0",
    "core-js": "^3.4.8",
    "chromedriver": "^79.0.0",
    "node-sass": "^4.13.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used",
          "argsIgnorePattern": "^_",
          "ignoreRestSiblings": false
        }
      ],
      "no-mixed-spaces-and-tabs": 0,
      "no-console": "off",
      "vue/max-attributes-per-line": 0,
      "vue/singleline-html-element-content-newline": 0,
      "vue/multiline-html-element-content-newline": 0,
      "no-undef": 0
    },
    "parserOptions": {
      "no-console": "off",
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
